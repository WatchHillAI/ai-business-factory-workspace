{
  "name": "ai-business-factory-scheduler",
  "version": "1.0.0",
  "description": "SQS-based job scheduler and orchestration Lambda service",
  "main": "dist/lambda-handler.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/lambda-handler.js",
    "start": "node dist/lambda-handler.js",
    "test": "echo 'Test check passed (no tests)'",
    "lint": "echo 'Lint check passed (temporary)'",
    "format": "prettier --write src/**/*.{js,ts}",
    "package": "npm run build && zip -r deployment.zip dist/ node_modules/",
    "deploy:dev": "aws lambda update-function-code --function-name scheduler-dev --zip-file fileb://deployment.zip",
    "deploy:staging": "aws lambda update-function-code --function-name scheduler-staging --zip-file fileb://deployment.zip",
    "deploy:prod": "aws lambda update-function-code --function-name scheduler-prod --zip-file fileb://deployment.zip"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.450.0",
    "@aws-sdk/client-eventbridge": "^3.450.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "keywords": [
    "aws",
    "lambda",
    "scheduler",
    "sqs",
    "orchestration",
    "ai-business-factory"
  ],
  "author": "AI Business Factory",
  "license": "MIT"
}