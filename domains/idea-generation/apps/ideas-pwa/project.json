{
  "name": "ideas-pwa",
  "root": "domains/idea-generation/apps/ideas-pwa",
  "sourceRoot": "domains/idea-generation/apps/ideas-pwa/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/ideas-pwa"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "port": 3002
      },
      "configurations": {
        "development": {
          "buildTarget": "ideas-pwa:build:development",
          "hmr": true
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "domains/idea-generation/apps/ideas-pwa/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "domains/idea-generation/apps/ideas-pwa"
      }
    }
  },
  "tags": ["scope:app", "type:app"]
}