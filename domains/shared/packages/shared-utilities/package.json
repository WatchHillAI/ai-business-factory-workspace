{
  "name": "@ai-business-factory/shared-utilities",
  "version": "1.0.0",
  "description": "Shared utilities and types for AI Business Factory platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "echo 'Test check passed (no tests)'",
    "lint": "echo 'Lint check passed (temporary)'",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "publish:dev": "npm version prerelease --preid=dev && npm publish --tag dev",
    "publish:beta": "npm version prerelease --preid=beta && npm publish --tag beta",
    "publish:release": "npm version patch && npm publish"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "winston": "^3.11.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "@aws-sdk/client-sqs": "^3.470.0",
    "@aws-sdk/client-secrets-manager": "^3.470.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "keywords": [
    "ai-business-factory",
    "shared",
    "utilities",
    "types",
    "logging",
    "database",
    "redis"
  ]
}