{
  "name": "ui-components",
  "root": "domains/shared/packages/ui-components",
  "sourceRoot": "domains/shared/packages/ui-components/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/ui-components",
        "main": "domains/shared/packages/ui-components/src/index.ts",
        "tsConfig": "domains/shared/packages/ui-components/tsconfig.lib.json",
        "generatePackageJson": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "domains/shared/packages/ui-components/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "domains/shared/packages/ui-components"
      }
    }
  },
  "tags": ["scope:shared", "type:ui"]
}