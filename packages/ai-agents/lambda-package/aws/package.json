{
  "name": "@ai-business-factory/ai-agents-lambda",
  "version": "1.0.0",
  "description": "AWS Lambda deployment package for AI Agent System",
  "main": "lambda-handler.js",
  "scripts": {
    "build": "tsc",
    "package": "npm run build && npm run zip",
    "zip": "zip -r ai-agents-lambda.zip dist/ node_modules/ package.json",
    "deploy": "aws lambda update-function-code --function-name ai-business-factory-ai-agent-orchestrator-dev --zip-file fileb://ai-agents-lambda.zip",
    "test": "jest",
    "clean": "rm -rf dist/ *.zip"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.450.0",
    "@aws-sdk/client-lambda": "^3.450.0",
    "@anthropic-ai/sdk": "^0.17.0",
    "openai": "^4.24.0",
    "axios": "^1.6.2",
    "redis": "^4.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0"
  },
  "keywords": [
    "aws-lambda",
    "ai-agents",
    "business-intelligence",
    "market-research"
  ],
  "author": "WatchHill AI",
  "license": "MIT"
}